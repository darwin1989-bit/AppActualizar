{
  "openapi": "3.0.1",
  "info": {
    "title": "Api_Login",
    "version": "1.0"
  },
  "paths": {
    "/api/Login/authenticate": {
      "post": {
        "tags": ["Login"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MLogin"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/MLogin"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/MLogin"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "MLogin": {
        "required": ["password", "userName"],
        "type": "object",
        "properties": {
          "userName": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
}
